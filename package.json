{
  "name": "bagtrack",
  "version": "0.4.1",
  "description": "Application built to help resellers track their profits and losses",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.grey.bagtrack",
    "productName": "BagTrack",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "0.4.0"
  },
  "author": "@obfuscation",
  "license": "MIT",
  "dependencies": {
    "@types/moment": "^2.13.0",
    "@types/react-datepicker": "^2.8.1",
    "@types/react-dom": "^16.8.5",
    "bootstrap": "^4.3.1",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-datepicker": "^2.8.0",
    "react-dom": "^16.8.6",
    "react-responsive-modal": "^4.0.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.4",
    "react-toastify": "^5.3.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^4.3.2",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.8.24",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-select": "^3.0.0",
    "@types/request": "^2.48.2",
    "@types/request-promise": "^4.1.44",
    "acorn": "^6.2.1",
    "ajv": "^6.10.2",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "electron": "^8.2.4",
    "electron-build": "^0.0.3",
    "electron-builder": "22.5.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "node-sass": "^7.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.18.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  }
}
